{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/pertanyaan",
            "dest": "/routes/PertanyaanRoute.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/users",
            "dest": "/routes/UserRoute.js",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "DELETE"
            ]
        }
    ]
}